<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/DECORATION/1.6.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/DECORATION/1.7.0 http://maven.apache.org/xsd/decoration-1.7.0.xsd"
	name="NIST Open Source Maven Parent">
	<bannerLeft>
		<name><![CDATA[
      <span class="color-highlight">Security Automation</span> Maven Project
      ]]>
		</name>
		<href>http://pages.nist.gov/oss-maven/</href>
	</bannerLeft>

	<publishDate position="bottom" format="yyyy-MM-dd" />
	<version position="left" />

	<skin>
	
<!--  		<groupId>io.github.devacfr.maven.skins</groupId> -->
<!--         <artifactId>reflow-maven-skin</artifactId> -->
<!--         <version>1.4.2</version> -->
		<groupId>gov.nist.secauto</groupId>
		<artifactId>reflow-maven-skin</artifactId>
		<version>${project.version}</version>
	</skin>
	<custom>
		<reflowSkin>
			<!-- Make this to 'false' for local development, i.e. file:// URLs -->
			<protocolRelativeURLs>false</protocolRelativeURLs>
<!-- 			<absoluteResourceURL>https://pages.nist.gov/oss-maven/</absoluteResourceURL> -->
			<smoothScroll>true</smoothScroll>
			<theme>bootswatch-spacelab</theme>
			<highlightJs>true</highlightJs>
			<highlightJsTheme>github</highlightJsTheme>
			<!-- <highlightJsVersion>9.12.0</highlightJsVersion> -->
			<!-- <bootstrapVersion>3.3.7</bootstrapVersion> -->
			<!-- <bootswatchVersion>3.3.7</bootswatchVersion> -->
			<skinAttribution>false</skinAttribution>
            <copyright>false</copyright>
			<brand>
				<name>
            <![CDATA[
            <span class="color-highlight">Security Automation</span> Maven Project
            ]]>
				</name>
				<href>http://pages.nist.gov/oss-maven/</href>
			</brand>
			<slogan>Default Maven build settings for NIST Security Automation projects</slogan>
			<titleTemplate>%2$s | %1$s</titleTemplate>
			<tocTopMax>6</tocTopMax>
			<!-- Include the documentation and tools in the top navigation (in addition 
				to links) -->
			<topNav>Documentation|Contribute|Maven Documentation</topNav>
			<!-- Split menus in the bottom navigation -->
			<bottomNav maxSpan="9">
				<column>Main</column>
				<column>Contribute</column>
                <column>Modules</column>
				<column>Maven Documentation</column>
			</bottomNav>
			<bottomDescription>
        <![CDATA[
          <span class="color-highlight">OSS Maven</span> provides for a consistent look-and-feel for
                project sites, and checks for serious coding issues, code style, and licenses in code.
        ]]>
			</bottomDescription>
            <markPageHeader>false</markPageHeader>
            <endContent>
			    <script src="https://pages.nist.gov/nist-header-footer/js/nist-header-footer.js" type="text/javascript"></script>
<!-- 			    <script async type="text/javascript" id="_fed_an_ua_tag" src="https://dap.digitalgov.gov/Universal-Federated-Analytics-Min.js?agency=NIST&subagency=github&pua=UA-42404149-54&yt=true&exts=ppsx,pps,f90,sch,rtf,wrl,txz,m1v,xlsm,msi,xsd,f,tif,eps,mpg,xml,pl,xlt,c"></script> -->
            </endContent>
			<pages>
                <index project="oss-maven">
                    <shortTitle>Security Automation Build Support</shortTitle>
                    <sections>
                        <body />
                    </sections>
                </index>
                <index project="oss-parent">
                    <shortTitle>Parent POM for Security Automation Projects</shortTitle>
                    <toc type="static">sidebar</toc>
                    <sections>
                        <body />
                    </sections>
                </index>
                <usage project="oss-parent">
                    <shortTitle>Use as a parent POM</shortTitle>
                    <sections>
                        <body />
                    </sections>
                </usage>
                <features project="oss-parent">
                    <shortTitle>Build features</shortTitle>
                    <sections>
                        <body />
                    </sections>
                </features>
                <eclipse project="oss-build-support">
                    <shortTitle>Eclipse Configuration</shortTitle>
                    <sections>
                        <body />
                    </sections>
                </eclipse>
			</pages>
		</reflowSkin>
	</custom>

	<body>
		<!-- Add a rel to Google profile for all pages -->
		<head>
		<![CDATA[
            <link rel="stylesheet" href="https://pages.nist.gov/nist-header-footer/css/nist-combined.css">
            <link rel="stylesheet" href="$resourcePath/css/custom.css">
        ]]>
		</head>

		<links>
			<item name="GitHub project"
				href="http://github.com/usnistgov/oss-maven/" />
		</links>

		<breadcrumbs>
			<item name="OSS Maven" href="./" />
		</breadcrumbs>

		<menu name="Main" inherit="top">
			<item name="Home" href="./" />
		</menu>
        <menu name="Documentation" inherit="top">
            <item name="Maven Usage" href="./oss-parent/usage.html"/>
            <item name="Build Features" href="./oss-parent/features.html"/>
            <item name="Eclipse Configuration" href="./oss-build-support/eclipse.html"/>
        </menu>
		<menu name="Contribute" inherit="bottom">
			<item name="Issues"
				href="http://github.com/usnistgov/oss-maven/issues" />
			<item name="Fork on GitHub"
				href="http://github.com/usnistgov/oss-maven/" />
            <item name="License" href="http://github.com/usnistgov/oss-maven/blob/master/LICENSE.md" />
		</menu>
        <menu name="Modules" ref="modules"/>
		<menu name="Maven Documentation" ref="reports" inherit="bottom" />
	</body>
</project>