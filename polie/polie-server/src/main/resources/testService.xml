<?xml version="1.0" encoding='utf-8'?>
<app:service xmlns:app="http://www.w3.org/2007/app"
	xmlns:rolie="urn:ietf:params:xml:ns:rolie-1.0" xmlns:atom="http://www.w3.org/2005/Atom"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="urn:ietf:params:xml:ns:rolie-1.0 rolie.xsd">
	<app:workspace>
		<atom:title>Public Facing Workspace</atom:title>
		<app:collection href="http://localhost:8080/polie-server/rest/feed/1">
			<atom:title>Incidents feed</atom:title>
			<atom:link rel="service"
				href="http://localhost:8080/polie-server/rest/service" />
			<app:categories fixed="yes">
				<atom:category scheme="urn:ietf:params:rolie:category:information-type"
					term="incidents" />
			</app:categories>
		</app:collection>
				<app:collection href="http://localhost:8080/polie-server/rest/feed/2">
			<atom:title>Indicators feed</atom:title>
			<atom:link rel="service"
				href="http://localhost:8080/polie-server/rest/service" />
			<app:categories fixed="no">
				<atom:category scheme="urn:ietf:params:rolie:category:information-type"
					term="indicators" />
			</app:categories>
		</app:collection>
	</app:workspace>
</app:service>